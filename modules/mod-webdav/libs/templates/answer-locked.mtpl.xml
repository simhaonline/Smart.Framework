<?xml version="1.0" encoding="utf-8"?>
<d:lockinfo xmlns:d="DAV:" xmlns:s="SMARTFRAMEWORKWEBDAV:"[####DAV-XML-PREFIX####]>
	<d:lockdiscovery>
		<s:info>
			Type: DAV/Lock
			Method: [####DAV-METHOD|html####]
			Path: [####DAV-REQ-PATH|html####]
			Owner: [####DAV-AUTHOR|html####]
		</s:info>
		<d:activelock>
			<d:locktype>
				<d:write/>
			</d:locktype>
			<d:lockscope>
				<d:exclusive/>
			</d:lockscope>
			<d:depth>[####LOCK-DEPTH|html####]</d:depth>
			<d:owner>[####DAV-AUTHOR|html####]</d:owner>
			<d:timeout>Second-[####LOCK-TIME-SEC|int####]</d:timeout>
			<d:locktoken>
				<d:href>urn:uuid:[####LOCK-UUID|html####]</d:href>
			</d:locktoken>
			<d:lockroot>
				<d:href>[####DAV-REQ-PATH|html####]</d:href>
			</d:lockroot>
		</d:activelock>
	</d:lockdiscovery>
</d:lockinfo>
